<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>로그인 화면</title>
</head>
<body>
	<div class="row">
		<form action="#" id="loginForm" onsubmit="return login();">
			<div class="col">
				<input class="form-control" type="text" name="memId" placeholder="아이디를 입력해주세요" required>
			</div>
			<div class="col">
				<input class="form-control" type="password" name="memPw" placeholder="비밀번호를 입력해주세요" required>
			</div>
			<div class="col text-end">
				<button type="submit" class="btn btn-dark">로그인</button>
			</div>
		</form>
	</div>
<script type="text/javascript" src="/resources/js/member/login.js"></script>
</body>
</html>