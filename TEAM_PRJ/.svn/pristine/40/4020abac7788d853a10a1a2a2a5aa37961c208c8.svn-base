@font-face {
	font-family: 'GowunDodum-Regular';
	src:
		url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_2108@1.1/GowunDodum-Regular.woff')
		format('woff');
	font-weight: normal;
	font-style: normal;
}

body {
	font-family: 'GowunDodum-Regular';
	overflow-x: hidden;
}

.bg-warning {
	background-color: #214099 !important;
	color: white !important;
}

.btn-primary {
	background-color: #214099 !important;
	border-color: #214099 !important;
	color: white !important;
}

.table tr:hover {
	background-color: #EEEEEE;
}

.table>:not(:first-child) {
	border-top: none;
}

.table>thead, .notice {
	border: 1px solid #DDDDDD;
}

.min-vh-80 {
	min-height: 80vh;
}

.page-item.active .page-link {
    background-color: #EEEEEE;
    border-color: #EEEEEE;
}

a {
	color: black !important;
}

a:hover {
	color: #AAAAAA !important;
}

.li-hover:hover {
	color: #AAAAAA !important;
}

.swal-modal {
	width: 500px;
}

.swal-button {
	background-color: #214099;
	font-size: 15px;
	text-shadow: 0px -1px 0px rgba(0, 0, 0, 0.3);
}

.swal-footer {
	text-align: center;
}

.swal-text {
	font-size: 18px;
}

.btn {
	margin-bottom: 0.25rem !important;
}